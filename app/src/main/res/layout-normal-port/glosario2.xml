<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blanco"

    fab:context="sistemasfireg.igp.org.sismosperu.Ultimosismo3">

<!--<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/blanco">
    -->

    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="0dp"
        android:paddingTop="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="0dp">



        <RelativeLayout android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/encabezado2">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <include
                    android:id="@+id/head"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@layout/encabezado"/>
            </RelativeLayout>
        </RelativeLayout>

    <RelativeLayout
        android:layout_height="60dp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingLeft="5dp"
        android:paddingRight="0dp"
        android:id="@+id/encabezado"
        android:background="@drawable/encabezadobackground">

        <RelativeLayout android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:gravity="center"
            android:layout_alignParentLeft="true"
            android:id="@+id/imagen1">
            <ImageView
                android:src="@drawable/logoigp"
                android:id="@+id/logoigp"
                style="@style/logoigp_normal_port2"
                android:layout_height="75dp"
                android:layout_alignParentLeft="true"
                android:gravity="center|center_vertical"
                android:layout_centerVertical="true"/>
        </RelativeLayout>

        <ImageView
            android:layout_height="65dp"
            style="@style/censislogo_normal_port"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:src="@drawable/centroencabezadoigp2"
            android:id="@+id/imageView2"
            android:layout_centerHorizontal="true" />
        <RelativeLayout android:layout_width="40dp"
            android:layout_height="60dp"
            android:layout_alignParentRight="true"
            android:id="@+id/imagen3">
            <Button
                android:background="@drawable/menuigp2"
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:id="@+id/button"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
    </RelativeLayout>




    <TextView
        android:text="@string/texto_glosario"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/encabezado2"
        android:id="@+id/texto"
        style="@style/titulolistadosismos" />
    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:layout_below="@+id/texto"
        android:background="@color/blanco"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/kk">
        <ScrollView android:layout_width="match_parent"
            android:layout_height="wrap_content"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:background="@color/blanco">
            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:orientation="vertical">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/uno"
                    android:orientation="horizontal"
                    android:paddingBottom="14dp"
                    android:paddingTop="14dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <TextView
                        android:id="@+id/titulomagnitudes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="Magnitudes:"
                        style="@style/titulo_glosario" />
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_below="@+id/titulomagnitudes"
                        android:layout_height="wrap_content"
                        android:id="@+id/intensidad1"
                        android:orientation="horizontal">
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccamarillo"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/icono1"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/circuloverde"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ccamarillo"
                                android:layout_gravity="center_vertical"
                                android:layout_centerVertical="true"
                                android:text="@string/PRIMERA_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_below="@+id/intensidad1"
                        android:layout_height="wrap_content"
                        android:id="@+id/intensidad2"
                        android:orientation="horizontal">
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccverde"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/icono2"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/circuloamarillo"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ccverde"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical"
                                android:text="@string/SEGUNDA_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_below="@+id/intensidad2"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccrojo"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/icono3"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/circulorojo"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ccrojo"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical"
                                android:text="@string/TERCERA_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/cuadroprofundidades"
                    android:orientation="horizontal"
                    android:layout_marginTop="6dp"
                    android:paddingBottom="7dp"
                    android:paddingTop="7dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/profundidadestitulo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="Profundidades (km):"
                            style="@style/titulo_glosario" />
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_below="@+id/profundidadestitulo"
                            android:id="@+id/superficiales"
                            android:layout_height="wrap_content">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/profccrojo"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/rojoproficono2"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/circulorojo"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/profrojodescripcion2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/profccrojo"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical"
                                android:text="Superficiales (0-60)"
                                style="@style/texto_glosario" />
                        </RelativeLayout>

                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_below="@+id/superficiales"
                            android:id="@+id/intermedio"
                            android:layout_height="wrap_content">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/profccverde"
                                android:minWidth="55dp">

                                <ImageView
                                    android:id="@+id/proficono2"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/circuloverde"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/profdescripcion2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/profccverde"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical"
                                android:text="Intermedio (61 - 300)"
                                style="@style/texto_glosario" />
                        </RelativeLayout>

                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_below="@+id/intermedio"
                            android:id="@+id/profundos"
                            android:layout_height="wrap_content">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/profccazul"
                                android:minWidth="55dp">

                                <ImageView
                                    android:id="@+id/proficonoazul"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/iconoazul"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/profdescripcionazul"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/profccazul"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical"
                                android:text="Profundos (> 300)"
                                style="@style/texto_glosario" />
                        </RelativeLayout>

                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_below="@+id/profundos"
                            android:id="@+id/ultimosismos"
                            android:layout_height="wrap_content">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ultimosismoamarillo"
                                android:minWidth="55dp">

                                <ImageView
                                    android:id="@+id/ulticonoamarillo"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/circuloamarillo"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/ultdescripcionamarillo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ultimosismoamarillo"
                                android:layout_centerVertical="true"
                                android:layout_gravity="center_vertical"
                                android:text="Último Sismo"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/cuatro"
                    android:orientation="horizontal"
                    android:layout_marginTop="6dp"
                    android:paddingBottom="7dp"
                    android:paddingTop="7dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/titulo4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="Fecha y hora local:"
                            style="@style/titulo_glosario" />
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/titulo4">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/cchora"
                                android:minWidth="55dp">
                                <TextView
                                    android:id="@+id/iconohora"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:textColor="@color/gris_claro"
                                    android:text="@string/hora"
                                    android:layout_centerHorizontal="true"
                                    android:gravity="center"
                                    android:padding="0dp"
                                    android:textSize="40sp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/cchora"
                                android:layout_gravity="center_vertical"
                                android:text="@string/FECHA_y_HORA_LOCAL_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/cinco"
                    android:orientation="horizontal"
                    android:paddingBottom="7dp"
                    android:layout_marginTop="6dp"
                    android:paddingTop="7dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/titulo5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="Magnitud:"
                            style="@style/titulo_glosario" />
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/titulo5">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccsismograma"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/icono5"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/sismograma3"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ccsismograma"
                                android:layout_gravity="center_vertical"
                                android:text="@string/MAGNITUD_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/seis"
                    android:orientation="horizontal"
                    android:paddingBottom="7dp"
                    android:layout_marginTop="6dp"
                    android:paddingTop="7dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/titulo6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="Referencia:"
                            style="@style/titulo_glosario" />

                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/titulo6">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccrefrencia"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/icono6"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/iconored"
                                    android:background="@color/blanco"
                                    android:padding="0dp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ccrefrencia"
                                android:layout_gravity="center_vertical"
                                android:text="@string/REFERENCIA_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/siete"
                    android:orientation="horizontal"
                    android:paddingBottom="7dp"
                    android:paddingTop="7dp"
                    android:layout_marginTop="6dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/titulo7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="Ubicación:"
                            style="@style/titulo_glosario" />
                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/titulo7">
                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccubicacion"
                                android:minWidth="55dp">
                                <TextView
                                    android:id="@+id/iconoubicacion"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_centerHorizontal="true"
                                    android:gravity="center"
                                    android:textColor="@color/gris_claro"
                                    android:text="@string/ubicacion"
                                    android:padding="0dp"
                                    android:textSize="36sp" />
                            </RelativeLayout>
                            <TextView
                                android:id="@+id/descripcion7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/ccubicacion"
                                android:layout_gravity="center_vertical"
                                android:text="@string/UBICACIÓN_TEXT"
                                style="@style/texto_glosario" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/ocho"
                    android:orientation="horizontal"
                    android:paddingBottom="7dp"
                    android:paddingTop="7dp"
                    android:layout_marginTop="6dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:background="@drawable/shapeborder">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/titulo8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="Profundidad:"
                            style="@style/titulo_glosario" />

                        <RelativeLayout android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/titulo8">

                            <RelativeLayout android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:minHeight="50dp"
                                android:layout_alignParentLeft="true"
                                android:layout_marginRight="8dp"
                                android:id="@+id/ccprofundidad"
                                android:minWidth="55dp">
                                <ImageView
                                    android:id="@+id/icono8"
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:layout_centerHorizontal="true"
                                    android:src="@drawable/profundidad"
                                    android:background="@color/blanco"
